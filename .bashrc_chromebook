# LINUXBREW
eval $(/home/andreas/.linuxbrew/bin/brew shellenv)

# LESS
export LESS="-M -i -R"

# Mount Dropbox file system
if [ -x $HOME/.local/bin/dbxfs ] && [ -d $HOME/mnt/dropbox ]; then
   [[ ! $(findmnt -M /home/andreas/mnt/dropbox) ]] && $HOME/.local/bin/dbxfs mnt/dropbox
fi


