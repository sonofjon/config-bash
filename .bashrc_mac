# Add /usr/local/sbin to PATH
PATH="${PATH}:/usr/local/sbin"

# Supress zsh new shell warning
export BASH_SILENCE_DEPRECATION_WARNING=1

#
# PROGRAMS
#

# BASE16 SHELL
BASE16_SHELL="$HOME/git/base16-shell/"
[ -n "$PS1" ] && \
    [ -s "$BASE16_SHELL/profile_helper.sh" ] && \
        eval "$("$BASE16_SHELL/profile_helper.sh")"

# ACTIVETCL
# PATH="/usr/local/bin:${PATH}"

# GNU CORE UTILITIES
PATH="/usr/local/opt/grep/libexec/gnubin:${PATH}"

# LINUXBREW
export HOMEBREW_UPDATE_REPORT_ONLY_INSTALLED=1

# LESS
export LESS="-M -i -R"

# PYTHON
PATH="/Library/Frameworks/Python.framework/Versions/3.9/bin:${PATH}"

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/andreas/opt/miniconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/andreas/opt/miniconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/andreas/opt/miniconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/andreas/opt/miniconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<
